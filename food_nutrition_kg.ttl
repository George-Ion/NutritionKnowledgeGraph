@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schema:AggregateRating a owl:DatatypeProperty ;
    rdfs:domain schema:Restaurant ;
    rdfs:range xsd:decimal .

schema:UserReview a owl:ObjectProperty ;
    rdfs:domain schema:Review ;
    rdfs:range schema:Recipe .

schema:calories a owl:DatatypeProperty ;
    rdfs:domain schema:Nutrition ;
    rdfs:range xsd:decimal .

schema:city a owl:DatatypeProperty ;
    rdfs:domain schema:Restaurant ;
    rdfs:range xsd:string .

schema:cookTime a owl:DatatypeProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range xsd:string .

schema:country a owl:DatatypeProperty ;
    rdfs:domain schema:Restaurant ;
    rdfs:range xsd:string .

schema:datePublished a owl:DatatypeProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range xsd:dateTime .

schema:fiberContent a owl:DatatypeProperty ;
    rdfs:domain schema:Nutrition ;
    rdfs:range xsd:decimal .

schema:givenName a owl:DatatypeProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range xsd:string .

schema:hasNutrition a owl:ObjectProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range schema:Nutrition .

schema:image a owl:DatatypeProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range xsd:anyURI .

schema:ingredients a owl:DatatypeProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range xsd:string .

schema:keywords a owl:DatatypeProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range xsd:string .

schema:legalName a owl:DatatypeProperty ;
    rdfs:domain schema:Restaurant ;
    rdfs:range xsd:string .

schema:name a owl:DatatypeProperty ;
    rdfs:domain schema:Nutrition ;
    rdfs:range xsd:string .

schema:orderDelivery a owl:DatatypeProperty ;
    rdfs:domain schema:Restaurant ;
    rdfs:range xsd:integer .

schema:priceRange a owl:DatatypeProperty ;
    rdfs:domain schema:Restaurant ;
    rdfs:range xsd:decimal .

schema:proteinContent a owl:DatatypeProperty ;
    rdfs:domain schema:Nutrition ;
    rdfs:range xsd:decimal .

schema:recipeCategory a owl:DatatypeProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range xsd:string .

schema:recipeIngredient a owl:ObjectProperty ;
    rdfs:domain schema:Recipe ;
    rdfs:range schema:Ingredient .

schema:reviewCount a owl:DatatypeProperty ;
    rdfs:domain schema:Review ;
    rdfs:range xsd:integer .

schema:reviewRating a owl:DatatypeProperty ;
    rdfs:domain schema:Review ;
    rdfs:range xsd:string .

schema:servesCuisine a owl:ObjectProperty ;
    rdfs:domain schema:Recipe,
        schema:Restaurant ;
    rdfs:range schema:Cuisine .

schema:sugarContent a owl:DatatypeProperty ;
    rdfs:domain schema:Nutrition ;
    rdfs:range xsd:decimal .

schema:Cuisine a owl:Class .

schema:Ingredient a owl:Class .

schema:Review a owl:Class .

schema:Nutrition a owl:Class .

schema:Restaurant a owl:Class .

schema:Recipe a owl:Class .

